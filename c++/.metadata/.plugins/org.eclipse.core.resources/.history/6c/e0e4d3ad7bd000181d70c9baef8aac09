#include <iostream>
using namespace std;

int main() {
int n,b;
long c;
cout<<"enter power:"<<endl;
cin>>n;
for(n>b;b++;){
c=2*2;
}

if(n>b){
cout<<c<<endl;
}










	return 0;
}
